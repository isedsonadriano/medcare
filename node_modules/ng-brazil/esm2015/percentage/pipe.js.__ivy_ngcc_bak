import { Pipe } from '@angular/core';
import { maskBr } from 'js-brasil';
export class PERCENTAGEPipe {
    transform(percentageValue) {
        return maskBr.porcentagem(percentageValue);
    }
}
PERCENTAGEPipe.decorators = [
    { type: Pipe, args: [{
                name: 'percentage',
            },] }
];
