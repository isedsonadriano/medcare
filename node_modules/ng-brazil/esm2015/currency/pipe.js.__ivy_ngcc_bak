import { Pipe } from '@angular/core';
import { maskBr } from 'js-brasil';
export class CURRENCYPipe {
    transform(currencyValue) {
        return maskBr.currency(currencyValue);
    }
}
CURRENCYPipe.decorators = [
    { type: Pipe, args: [{
                name: 'currencyBrazil',
            },] }
];
